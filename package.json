{
  "name": "eslint-plugin-react-snob",
  "version": "0.0.15",
  "description": "An ESLint plugin for React best practices",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "react",
    "typescript"
  ],
  "author": "Jason Paff <jason@jasonpaff.com>",
  "homepage": "https://github.com/jasonpaff/eslint-plugin-react-snob#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonpaff/eslint-plugin-react-snob.git"
  },
  "bugs": {
    "url": "https://github.com/jasonpaff/eslint-plugin-react-snob/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./configs/flat": "./dist/configs/flat.js"
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.40.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript-eslint/rule-tester": "^8.40.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "jest": "^29.5.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0 || >=9.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT"
}
